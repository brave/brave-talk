    "i18next": "23.16.8",
    "jest-environment-jsdom": "29.7.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-i18next": "14.1.3"
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write",
    "*.ts?(x)": "eslint"
  },
  "resolutions": {
    "svelte": "4.2.20"
  }
